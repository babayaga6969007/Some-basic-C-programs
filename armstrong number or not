#include<stdio.h>
#include<conio.h>

int main()
{
int a,b,c,num,sum,n;
printf("enter a three digit number");
scanf("%d",&num);
n=num;
a=num%10;
num=num/10;
b=num%10;
num=num/10;
c=num%10;

sum=a*a*a+b*b*b+c*c*c;
if(sum == n)
{
printf("it is a armstrong number");
}
else
{
printf("it is not a armstrong number");
}
return 0;
}
