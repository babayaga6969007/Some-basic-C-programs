#include<stdio.h>
#include<conio.h>
int main()
{
int a,b;
printf("enter two numbers to swap");
scanf("%d%d",&a,&b);
printf("Before swappiong,value = %d and %d,a,b);
printf("After swapping, value = %d and %d,b,a);
getch();
}

//Another method
#include<stdio.h>
#include<conio.h>
int main()
{
int a,b;
printf("enter two numbers");
scanf("%d%d",&a,&b);
printf("the value before swap is %d and %d",a,b);
a=a+b;
b=a-b;
a=a-b;
printf("the swapped value is %d and %d",a,b);
getch();
}
//
