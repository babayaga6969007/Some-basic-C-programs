#include<stdio.h>
#include<conio.h>
int main()
{
int id,password;
printf("Enter id");
scanf("%d",&id);
switch(id)
   {
    case 20:       
    printf("enter password");
    scanf("%d",&password);
    switch(password)
    {
    case 69:
        printf("welcome admin");
        break;
    default:
        printf("sorry,wrong password");
        break;
    }

   break;
    default:
        printf("incorrect id");
        break;
   }

   getch();

}
//the default id and password is 20 and 69 respectively.You will get an error message if anyone of them is wrong.
